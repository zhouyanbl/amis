  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "src/**/*"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "setupFiles": [],
    "testRegex": "/.*\\.test\\.(ts|tsx|js)$",
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    }
  },
  "_husky": {
    "hooks": {
